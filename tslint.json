{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [true, { "multiline": "always", "singleline": "never" }],
        "prefer-const": true,
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "max-line-length": [true, 140]
    },
    "rulesDirectory": []
}