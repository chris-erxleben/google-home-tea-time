{
  "name": "google-home-webhook",
  "version": "1.0.0",
  "description": "Simple Google Home webhook app",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "postbuild": "run-s clean lint tsc",
    "start:watch": "nodemon --watch src -e ts --exec run-s postbuild start",
    "start": "node .",
    "test:watch": "jest --watch",
    "test": "jest",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chris-erxleben/google-home-webhook.git"
  },
  "keywords": [
    "google",
    "home",
    "api",
    "webhook",
    "dialogflow"
  ],
  "author": "Chris Erxleben <cerxleben.fhs@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chris-erxleben/google-home-webhook/issues"
  },
  "homepage": "https://github.com/chris-erxleben/google-home-webhook#readme",
  "dependencies": {
    "@types/express": "^4.16.1",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "jest": "^23.0.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "supertest": "^3.4.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4"
  }
}
